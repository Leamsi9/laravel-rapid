{
    "name": "leamsi9/rapid",
    "description": "A CLI driven Minimum Viable Website and micro-CMS generator.",
    "keywords": ["CMS", "Website", "Scaffolding", "Laravel", "Blog", "Generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ismael Velasco",
            "email": "ismael@ismaelvelasco.dev"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "laravel/framework": "^7.0||^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rapid\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Rapid\\WinkServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "Rapid\\ComposerInstallMessage::postPackageInstall"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
